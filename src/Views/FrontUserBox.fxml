<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="nav" prefHeight="700.0" prefWidth="284.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.FrontUserBoxController">
   <children>
      <Label fx:id="myProfile" onMouseEntered="#highlightMyProfile" onMouseExited="#unhighlightMyProfile" prefHeight="43.0" prefWidth="233.0" text="My Profile" textAlignment="CENTER">
         <VBox.margin>
            <Insets left="30.0" right="30.0" top="50.0" />
         </VBox.margin>
         <opaqueInsets>
            <Insets left="10.0" />
         </opaqueInsets>
         <padding>
            <Insets left="10.0" />
         </padding>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
      <Label fx:id="editData" contentDisplay="CENTER" layoutX="40.0" layoutY="60.0" onMouseClicked="#getContent" onMouseEntered="#highlightEdit" onMouseExited="#unhighlightEdit" prefHeight="43.0" prefWidth="233.0" text="Edit My Profile" textAlignment="CENTER">
         <VBox.margin>
            <Insets left="30.0" right="30.0" top="10.0" />
         </VBox.margin>
         <padding>
            <Insets left="10.0" />
         </padding>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
      <Label fx:id="favoris" contentDisplay="CENTER" layoutX="40.0" layoutY="113.0" onMouseClicked="#getContent" onMouseEntered="#highlightFavoris" onMouseExited="#unhighlightFavoris" prefHeight="43.0" prefWidth="233.0" text="Manage My Favourites" textAlignment="CENTER" visible="false">
         <VBox.margin>
            <Insets left="30.0" right="30.0" top="10.0" />
         </VBox.margin>
         <padding>
            <Insets left="10.0" />
         </padding>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
      <Label fx:id="myPhotos" contentDisplay="CENTER" layoutX="40.0" layoutY="166.0" onMouseClicked="#getContent" onMouseEntered="#highlightPhotos" onMouseExited="#unhighlightPhotos" prefHeight="43.0" prefWidth="233.0" text="My Photos" textAlignment="CENTER" visible="false">
         <VBox.margin>
            <Insets left="30.0" right="30.0" top="10.0" />
         </VBox.margin>
         <padding>
            <Insets left="10.0" />
         </padding>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
   </children>
</VBox>
